    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "cross-env NODE_ENV=development nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "cross-env NODE_ENV=production node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./e2e/jest-e2e.json",
        "documentation:serve": "npx @compodoc/compodoc -p tsconfig.json --serve",
        "prisma:push": "dotenv -e ./env/development.env -- npx prisma db push --schema ./prisma/schema.prisma",
        "prisma:pull": "dotenv -e ./env/development.env -- npx prisma db pull --schema ./prisma/schema.prisma",
        "prisma:migrate:dev": "dotenv -e ./env/development.env -- npx prisma migrate dev",
        "prisma:migrate:reset": "dotenv -e ./env/development.env -- npx prisma migrate reset",
        "prisma:migrate:prod": "dotenv -e ./env/development.env -- npx prisma migrate deploy",
        "prisma:studio": "dotenv -e ./env/development.env prisma studio --schema ./prisma/schema.prisma",
        "prisma:seed": "dotenv -e ./env/development.env npx prisma db seed",
        "prisma:generate": "prisma generate --schema ./prisma/schema.prisma"
    },

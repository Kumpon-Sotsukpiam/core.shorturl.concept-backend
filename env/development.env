# Environment variables declared in this file are automatically made available to Prisma.
# See the documentation for more detail: https://pris.ly/d/prisma-schema#accessing-environment-variables-from-the-schema

# Prisma supports the native connection string format for PostgreSQL, MySQL, SQLite, SQL Server, MongoDB (Preview) and CockroachDB (Preview).
# See the documentation for all the connection string options: https://pris.ly/d/connection-strings
# PostgreSQL
DATABASE_URL="postgresql://admin:admin@192.168.1.4:5433/kutt?schema=public"
POSTGRES_HOST=192.168.1.4
POSTGRES_PORT=5433
POSTGRES_USER=admin
POSTGRES_PASSWORD=admin
POSTGRES_DB=kutt

# Swagger Dark Mode
SWAGGER_DARK=true
# Redis ENV
REDIS_HOST=192.168.1.4
REDIS_PORT=6379
REDIS_DB=0

# Security
JWT_ACCESS_SECRET=access-security-key
JWT_ACCESS_EXPIRATION_TIME=60000
BCRYPT_SALT_OR_ROUND=10

GRAPHQL_PLAYGROUND=1
PORT=3001

# Authentication ENV
DISALLOW_REGISTRATION=true

# Links
LINK_LENGTH=6
DEFAULT_DOMAIN=localhost:3000